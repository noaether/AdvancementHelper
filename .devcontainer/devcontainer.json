{
  "name": "AH",
  "image": "mcr.microsoft.com/devcontainers/universal:2",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "21",
      "jdkDistro": "amzn",
      "installGradle": "false",
      "installMaven": "true",
      "mavenVersion": "latest"
    }
  },
  "customizations": {
    "codespaces": {
      "repositories": {
        "noaether/*": {
          "permissions": {
            "permissions": "write-all"
          }
        }
      }
    }
  }
}
